{
  "author": "mubaidr@gmail.com",
  "bugs": {
    "url": "https://github.com/mubaidr/Javascript-Barcode-Reader/issues"
  },
  "description": "Barcode reader solution in Javascript for Browser and Node.js",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "parcel-bundler": "^1.9.7",
    "rimraf": "^2.6.2"
  },
  "files": [
    "src",
    "dist"
  ],
  "homepage": "https://github.com/mubaidr/Javascript-Barcode-Reader#readme",
  "keywords": [
    "barcode",
    "reader",
    "scanner",
    "javascript",
    "node"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "name": "javascript-barcode-reader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mubaidr/Javascript-Barcode-Reader.git"
  },
  "scripts": {
    "build": "parcel build src/index.js --out-dir dist --out-file javascript-barcode-reader",
    "build-docs": "rimraf docs/ && parcel build docs-src/index.html --out-dir docs --public-url ./",
    "dev": "cross-env NODE_ENV=development && parcel serve docs-src/index.html --out-dir .cache",
    "lint": "eslint --fix src/"
