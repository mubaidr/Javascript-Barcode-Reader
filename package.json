{
  "author": "Muhammad Ubaid Raza <mubaidr@gmail.com>",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "jimp": "^0.22.12"
  },
  "description": "Simple & Fast Barcode decoder for Browser and Node.js",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/babel__core": "^7.1.14",
    "@types/babel__preset-env": "^7.1.14",
    "@types/core-js": "^2.5.8",
    "@types/eslint": "^8.56.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/prettier": "^2.7.3",
    "@types/rollup-plugin-json": "^3.0.3",
    "@types/semantic-release": "^17.2.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "commitizen": "^4.3.1",
    "core-js": "^3.40.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "rollup": "^4.30.1",
    "@rollup/plugin-json": "^6.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "semantic-release": "^24.2.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src",
    "dist"
  ],
  "homepage": "https://github.com/mubaidr/Javascript-Barcode-Reader#readme",
  "keywords": [
    "barcode",
    "reader",
    "scanner",
    "javascript",
    "node"
  ],
  "license": "MIT",
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/javascript-barcode-reader.umd.min.js",
  "module": "dist/javascript-barcode-reader.es5.min.js",
  "name": "javascript-barcode-reader",
  "repository": {
    "type": "git",
    "url": "git://github.com/mubaidr/Javascript-Barcode-Reader.git"
  },
  "scripts": {
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --options typedoc.json",
    "commit": "git-cz",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "lint": "eslint --ext .js,.ts src test --fix && prettier --write \"{test,src,tools}/**/*.{js,ts}\"",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "test:watch": "jest --watch"
  },
  "typings": "dist/types/src/index.d.ts",
  "version": "0.6.9"
}
